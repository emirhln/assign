#include <stdio.h>
#include <stdlib.h>
int main()
{
int x;
printf("enter a positive weather condition:");
scanf("%d",&x);
switch (x) {
    case 1:
    case 2:
    case 3:
    case 4:
    printf("Clear Skies.\n");
    break;
    case 5:
    case 6:
    case 7:
    case 8:
    printf("Partly Cloudly.\n");
    break;
    case 9:
    case 10:
    case 11:
    case 12:
    printf("Overcast\n");
    break;
    default:
    printf("Rainy.\n");
}
    return 0;
}
    
